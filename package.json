{
  "name": "freik-local-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "config": "cmake -B build -G \"Ninja Multi-Config\" -S . -DCMAKE_PROJECT_TOP_LEVEL_INCLUDES=cmake/conan_provider.cmake",
    "rbuild": "cmake --build build --config Release",
    "dbuild": "cmake --build build --config Debug",
    "wdbld": "bun build ./www/index.html --outdir=build/Debug/www --sourcemap --target=browser --define:process.env.NODE_ENV='\"debug\"' --env='BUN_PUBLIC_*'",
    "wrbld": "bun build ./www/index.html --outdir=build/Release/www --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "format": "tools format bun",
    "prepare": "husky",
    "bld": "bun run scripts/build.ts",
    "dbld": "bun run scripts/build.ts Debug",
    "rbld": "bun run scripts/build.ts Release"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.64.0",
    "jotai": "^2.12.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@freik/workspace": "^0.6.5",
    "@types/bun": "latest",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "clang-format": "^1.8.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3"
  }
}
